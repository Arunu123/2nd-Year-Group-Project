CREATE TABLE `contactdetails` (
  `GuestName` varchar(50) NOT NULL ,
  `email` varchar(255) NOT NULL,
  `Message` varchar(255) NOT NULL,
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `Timestamp` timestamp NOT NULL DEFAULT current_timestamp(),
     PRIMARY KEY (`id`)
) ;

